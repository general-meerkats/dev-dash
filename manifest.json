{
    "manifest_version": 2,

    "name": "dev-dash",
    "description": "GitHub repo activity on new tab",
    "version": "1.0.2",
        
    "browser_action": {
        "default_icon": "dist/assets/favicon.png",
        "default_popup": "popup.html"
    },

    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    
    "icons": {
        "128": "dist/assets/favicon.png"
    }
}